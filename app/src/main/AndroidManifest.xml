<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jether.wirentiuserapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_john_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_john_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Wirenti"
        android:usesCleartextTraffic="true">
        <activity android:name=".UpdateActivity"></activity>
        <activity android:name=".NedaActivity" />
        <activity android:name=".NoInternetActivity" />
        <activity android:name=".GezakoActivity" />
        <activity android:name=".EditPropertyActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".UpdatePropertyActivity" />
        <activity android:name=".FeaturedImageActivity" />
        <activity android:name=".LogOutActivity" />
        <activity android:name=".AddPropertyActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".MyPropertiesActivity" />
        <activity android:name=".PropertyDetailsOwnerActivity" />
        <activity android:name=".PropertyDetailsActivity" />
        <activity android:name=".BottomActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".OnBoardWalkingActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>